#!/bin/sh

SCRIPT_DIR="$(dirname $(readlink -f "$0"))"
if [ -f $SCRIPT_DIR/includes ]; then
  . $SCRIPT_DIR/includes
fi

# Make sure git is up to date
echo "Updating git branches - dev, stage, prod" | toLogs

git fetch --all -q
git checkout dev -q
git pull -q
git checkout stage -q
git pull -q
git checkout prod -q
git pull -q

return 0
