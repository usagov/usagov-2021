#!/bin/sh

SCRIPT_DIR="$(dirname $(readlink -f "$0"))"
if [ -f $SCRIPT_DIR/includes ]; then
  . $SCRIPT_DIR/includes
fi

assertRequires cf

echo "Current CMS Deploy:" | toLogs
cf ssh cms -c "cat /etc/motd" | toLogs

echo "Current WAF Deploy:" | toLogs
cf ssh waf -c "cat /etc/motd" | toLogs

return 0
