#!/bin/sh
#
# This script will attempt to store a container image on docker hub
# to be used when launching cloud.gov images
#

CONTAINERTAG=${1}
CONTAINERTAG2=${2}

if [ -z "$CONTAINERTAG" ]
then
      echo "Must specify a container tag to push"
      exit 1;
fi;

docker push ednark/usagov-2021:$CONTAINERTAG

if [ -n "$CONTAINERTAG2" ]
then
    docker push ednark/usagov-2021:$CONTAINERTAG2
fi

# docker search ednark/usagov-2021:$CONTAINERTAG

