{% set test = element['#object'].body[0].value|raw %}

{% if "Based on your answers" in test or "Basándose en sus respuestas" in test %}
<div class="usa-prose">
{% if element["#language"] == "en" %}
    <p class="usa-scam-label">WHERE TO REPORT A SCAM</p>
{% else %}
    <p class="usa-scam-label">Dónde reportar una estafa</p>
{% endif %}
</div>
<div class="maxw-tablet margin-x-auto">
    {% for item in items %}
      <div{{ item.attributes }}>{{ item.content }}</div>
    {% endfor %}
</div>
     {% if element["#language"] == "en" %}
        <div class="grid-row flex-justify-center maxw-mobile margin-x-auto margin-top-6">
            <a class="usa-button" href="/where-report-scams#block-usagov-content">
                Start over
            </a>   
        </div>
        {% elseif element["#language"] == "es" %}
        <div class="grid-row flex-justify-center maxw-mobile margin-x-auto margin-top-6">
            <a class="usa-button" href="/es/donde-reportar-una-estafa#block-usagov-content">
                Empezar
            </a>   
        </div>
        {% endif %}
{% endif %}
