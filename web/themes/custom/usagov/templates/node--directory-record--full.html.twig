{{content.field_page_intro}}
{{content.body}}
<div class="usagov-directory-table">
  <div>
    <h3>Website</h3>
    {{ content.field_website }}
  </div>

  <div>
    <h3>Contact</h3>
    {{ content.field_contact_link }}
  </div>

  {% if content.field_phone_number|render %} 
  <div>
    <h3>Phone Number</h3>
    <p>{{content.field_phone_number}}</p>
  </div>
  {% endif %}

  {% if content.field_toll_free_number|render %} 
  <div>
    <h3>Toll-Free Number</h3>
    <p>{{content.field_toll_free_number}}</p>
  </div>
  {% endif %}

  {% if content.field_tty_number|render %} 
  <div>
    <h3>TTY</h3>
    <p>{{content.field_tty_number}}</p>
  </div>
  {% endif %}

  {% if content.field_email|render %} 
  <div>
    <h3>Email</h3>
    <p>{{content.field_email}}</p>
  </div>
  {% endif %}

  {% if content.field_offices_near_you|render %}
  <div>
    <h3>Find an Office Near You</h3>
    {{content.field_offices_near_you}}
  </div>
  {% endif %}

  {% if content.group_main_address %}
    {% set address = content.group_main_address %} 
  <div>
    <h3>Main Address</h3>
    {{ address.field_street_1 }}<br>
    {% if address.field_street_2|render %}
    {{ address.field_street_2 }}<br>
    {% endif %}
    {% if address.field_street_3|render %}<br>
    {{ address.field_street_3 }}
    {% endif %}      
    {# {{ address.field_city }}, {{ address.field_state_abbr }}  {{ address.field_zip }} #}
  </div>
  {% endif %}
</div>
