<style>
	.wrapper {
		display: grid;
		grid-template-columns: 1fr 1fr;
		/*   grid-template-rows: repeat(4, auto); */
	}

	#banner {
		grid-column-start: 1;
		grid-column-end: 3;
		grid-row-start: 1;
		grid-row-end: 3;
 		background-image: url({{file_url(node.field_navigation_banner_image.entity.field_media_image.entity.fileuri)}});
		background-size: cover;
		background-position: center;
	}
	#intro {
		grid-column-start: 2;
		grid-column-end: 3;
		grid-row-start: 1;
		grid-row-end: 2;
		background: #112F4E;
		color: white;
		margin: 40px;
	}

	#links {
		grid-column-start: 1;
		grid-column-end: 3;
		grid-row-start: 2;
		grid-row-end: 4;
		z-index: 1;
		background-color: white;
		margin: 40px;
	}
	#life-events-wrapper {
		grid-column-start: 1;
		grid-column-end: 3;
		grid-row-start: 3;
		grid-row-end: 5;
		background-color: #112F4E;
	}
	#life-events {
		grid-column-start: 1;
		grid-column-end: 3;
		grid-row-start: 4;
		grid-row-end: 5;
		color: white;
		margin: 40px;
	}
	.wrapper {
		background-color: #fff4e6;
	}

	.box {
		/*   margin: 40px; */
		padding: 20px;
		box-shadow: 5px 5px 5px rgb(0 0 0 / 0.2);
		border-radius: 5px;
	}
</style>
