{{ content.field_page_intro }}
{{ content.body }}
<div class="usagov-directory-table">
  <div>
    <h3>State Government Website</h3>
    {{ content.field_website }}
  </div>

  <div>
    <ul class="usa-list usa-list--unstyled">
      <h3>Governor</h3>
      {{ content.field_governor }}<br>
      {{ content.field_governor_contact }}
      <p class="usagov-dir-padding">Phone: {{ content.field_phone_number }}</p>
      {% if content.group_main_address %}
      {% set address = content.group_main_address %}
        <p class="usagov-dir-padding">Main Address:</p>
        {{ address.field_street_1 }}
        {% if address.field_street_2|render %}
        <br>{{ address.field_street_2 }}
        {% endif %}
        {% if address.field_street_3|render %}
        <br>{{ address.field_street_3 }}
        {% endif %}
        {% if address.field_city|render %}
        {{ address.field_city }}, 
        {% endif %}
        {% if address.field_state_abbr or address.field_zip %} 
        {{ address.field_state_abbr }}  {{ address.field_zip }}
        {% endif %}
      {% endif %}
    </ul>
  </div>

  <div>
    <h3>Congress Members</h3>
    <a href="/elected-officials">Find the names and contact information for your elected officials</a>
  </div>

  <div>
    <h3>Offices in Your State</h3>
    {{ content.field_social_services }}<br>
    {{ content.field_district_attorneys }}<br>
    {{ content.field_consumer_protection }}<br>

    {% if content.field_corrections_dept|render %}
    {{ content.field_corrections_dept }}<br>
    {% endif %}
    
    {{ content.field_local_governments }}<br>
    {{ content.field_education_department }}<br>
    {{ content.field_emergency_management }}<br>

    {% if content.field_election_office|render %}
    {{ content.field_election_office }}<br>
    {% endif %}

    {{ content.field_health_department }}<br>
    {{ content.field_motor_vehicle_office }}
  </div>

</div>
