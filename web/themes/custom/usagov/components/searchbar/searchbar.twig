<form class={{form_classes}} action={{action}} method="get" name="search_form" accept-charset="UTF-8" role="search">
  <label id="{{label_id}}" class="usa-sr-only" for="{{for_string}}">{{translations.agencySearchLabel}}</label>
  <input id="{{hidden_input_id}}" name="{{hidden_name_string}}" type="hidden" value="{{translations.agencySearchAffiliateID}}">
  <input
      class={{input_classes}}
      id="{{input_id}}"
      aria-labelledby="{{aria_labelledby_string}}"
      type="search"
      name="query"
      placeholder="{{translations.agencySearchLabel}}"
      onfocus="this.placeholder"
      autocomplete="off"
  />
  <button class={{button_classes}} type="submit">
    <img src="/themes/custom/usagov/images/search--dark.svg" alt="{{translations.search}}">
  </button>
</form>
