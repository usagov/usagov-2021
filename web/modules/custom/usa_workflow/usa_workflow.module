<?php
function usa_workflow_form_alter(&$form, \Drupal\Core\Form\FormStateInterface $form_state, $form_id){ 
  $newform = ['node_basic_page_form', 'node_wizard_step_form', 'node_wizard_form'];
  if(in_array($form_id,$newform)){
    dpm($form_id);
    unset($form['moderation_state']['widget'][0]['state']['#options']['published']);
  }
}