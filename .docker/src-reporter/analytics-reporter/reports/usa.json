    {
      "name": "top-cities-90-days",
      "frequency": "daily",
      "query": {
        "dimensions": [
          {
            "name": "city"
          }
        ],
        "metrics": [
          {
            "name": "totalUsers"
          }
        ],
        "dateRanges": [
          {
            "startDate": "90daysAgo",
            "endDate": "yesterday"
          }
        ],
        "orderBys": [
          {
            "metric": {
              "metricName": "totalUsers"
            },
            "desc": true
          }
        ],
        "dimensionFilter": {
          "andGroup": {
            "expressions": [
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "city",
                    "stringFilter": {
                      "value": "(other)",
                      "caseSensitive": false
                    }
                  }
                }
              },
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "city",
                    "stringFilter": {
                      "value": "null",
                      "caseSensitive": false
                    }
                  }
                }
              },
              {
                "notExpression": {
                  "filter": {
                    "fieldName": "city",
                    "stringFilter": {
                      "value": "",
                      "caseSensitive": false
                    }
                  }
                }
              }
            ]
          }
        }
      },
      "meta": {
        "name": "Top Cities (90 Days)",
        "description": "Last 90 days' of Top cities for onsite users."
      }
    },
