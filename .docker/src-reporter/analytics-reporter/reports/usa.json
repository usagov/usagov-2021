{
  "reports": [
    {
      "name": "devices",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": [
          {
            "name": "date"
          },
          {
            "name": "deviceCategory"
          }
        ],
        "metrics": [
          {
            "name": "sessions"
          }
        ],
        "dateRanges": [
          {
            "startDate": "90daysAgo",
            "endDate": "yesterday"
          }
        ],
        "orderBys": [
          {
            "dimension": {
              "dimensionName": "date"
            },
            "desc": true
          }
        ]
      },
      "meta": {
        "name": "Devices",
        "description": "90 days of desktop/mobile/tablet visits for all sites."
      }
    },
