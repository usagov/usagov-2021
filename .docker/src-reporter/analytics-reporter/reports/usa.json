        "dimensions": ["ga:exitPagePath"],
        "metrics": ["ga:sessions", "ga:pageviews", "ga:users", "ga:pageviewsPerSession", "ga:avgSessionDuration", "ga:exits"],
        "start-date": "30daysAgo",
        "end-date": "yesterday",
