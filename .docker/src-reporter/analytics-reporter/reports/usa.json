    {
      "name": "top-external-links-yesterday",
      "frequency": "hourly",
      "query": {
        "dimensions": [
          {
            "name": "pageTitle"
          },
          {
            "name": "linkUrl"
          },
          {
            "name": "linkDomain"
          },
          {
            "name": "pagePath"
          }
        ],
        "metrics": [
          {
            "name":"eventCount"
          }
        ],
        "dateRanges": [
          {
            "startDate": "yesterday",
            "endDate": "yesterday"
          }
        ],
        "orderBys": [
          {
            "metric": {
              "metricName": "eventCount"
            },
            "desc": true
          }
        ],
        "dimensionFilter": {
          "filter": {
            "fieldName": "eventName",
            "stringFilter": {
              "value": "click"
            }
          },
          "filter": {
            "fieldName": "outbound",
            "stringFilter": {
              "value": "true"
            }
          }
        }
      },
      "meta": {
        "name": "All external links yesterday",
        "description": "Most popular external links clicked on all sites yesterday."
      }
