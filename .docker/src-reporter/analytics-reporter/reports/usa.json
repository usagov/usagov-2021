    {
      "name": "os",
      "frequency": "daily",
      "slim": true,
      "query": {
        "dimensions": [
          {
            "name": "date"
          },
          {
            "name": "operatingSystem"
          }
        ],
        "metrics": [
          {
            "name": "sessions"
          }
        ],
        "dateRanges": [
          {
            "startDate": "90daysAgo",
            "endDate": "yesterday"
          }
        ],
        "metricFilter": {
          "filter": {
            "fieldName": "sessions",
            "numericFilter": {
              "operation": "GREATER_THAN",
              "value": {
                "int64Value": "10"
              }
            }
          }
        },
        "sort": "date"
      },
      "meta": {
        "name": "Operating Systems",
        "description": "90 days of visits, broken down by operating system and date, for all sites. (>100 sessions)"
      }
    },
